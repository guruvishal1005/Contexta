# API Configuration
# Copy this file to .env.local and update values as needed

# Backend API URL (without trailing slash)
NEXT_PUBLIC_API_URL=http://localhost:8000/api/v1

# Backend base URL (for health checks, docs)
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000

# Local storage keys for authentication tokens
NEXT_PUBLIC_TOKEN_KEY=contexta_access_token
NEXT_PUBLIC_REFRESH_TOKEN_KEY=contexta_refresh_token
